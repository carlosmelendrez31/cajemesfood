@model Usuariomodel
@{
    ViewData["Title"] = "Editar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
<div class=" card-header">editar contacto</div>
<div class="card-body">
    <form asp-action="Editar" asp-controller="Mantenedor" method="post">
        <input asp-for="id_usuario" type="hidden" class="form-control">
        <div class="mb-3">
            <label class="form-label">Nombre</label>
            <input asp-for="Nombre" type="text" class="form-control">
        </div>
        <div class="mb-3">
            <label class="form-label">Contraseña</label>
            <input asp-for="contrasena" type="password" class="form-control">
        </div>
        <div class="mb-3">
            <label class="form-label">Telefono</label>
            <input asp-for="Telefono" type="text" class="form-control">
        </div>

        <button type="submit" class="btn btn-primary">editar</button>
        <a>Volver a la lista</a>

    </form>
</div>
</div>
