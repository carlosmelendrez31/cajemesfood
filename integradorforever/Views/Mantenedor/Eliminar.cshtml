@model Usuariomodel
@{
    ViewData["Title"] = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class=" card-header">Eliminar usuario</div>
    <div class="card-body">
        <form asp-action="Eliminar" asp-controller="Mantenedor" method="post">
            <input asp-for="id_usuario" type="hidden" class="form-control">
            <div class="alert alert-danger" role="alert">
                Desea eliminar el contacto : @Html.DisplayTextFor(m => m.Nombre)?
            </div>
           

            <button type="submit" class="btn btn-danger">ELIMINAR</button>
            <a>Volver a la lista</a>

        </form>
    </div>
</div>

